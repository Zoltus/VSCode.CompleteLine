{
  "name": "completeline",
  "displayName": "Complete Line",
  "description": "Completes a line of code",
  "version": "1.0.1",
  "publisher": "refactor11",
  "author": {
    "name": "Jakob Christensen"
  },
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "Complete",
    "Line",
    "Snippets"
  ],
  "license": "SEE LICENSE IN LICENSE.txt",
  "activationEvents": [
    "onCommand:extension.completeLine"
  ],
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/JakobChristensen/VSCode.CompleteLine.git"
  },
  "homepage": "https://github.com/JakobChristensen/VSCode.CompleteLine",
  "contributes": {
    "commands": [
      {
        "command": "extension.completeLine",
        "title": "Complete Line"
      }
    ],
    "keybindings": [
      {
        "command": "extension.completeLine",
        "key": "ctrl+shift+enter",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.71.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
